{
  "schemaVersion": 1,
  "id": "mod_id",
  "version": "${version}",
  "name": "Name Here",
  "description": "Mod Description",
  "authors": [
    "Author"
  ],
  "contact": {
    "homepage": "https://example.com/",
    "issues": "https://example.com/",
    "sources": "https://example.com/"
  },
  "license": "Licence",
  "icon": "assets/mod_id/icon.png",
  "environment": "*",
  "languageAdapters": {
    "custom": "com.username.modid.language_adapter.CustomAdapter"
  },
  "entrypoints": {
    "main": [
      {
        "adapter": "custom",
        "value": "com.username.modid.MainFile"
      }
    ],
    "client": [
      {
        "adapter": "custom",
        "value": "com.username.modid.MainFileClient"
      }
    ]
  },
  "mixins": [
    "mod_id.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.7",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  }
}
